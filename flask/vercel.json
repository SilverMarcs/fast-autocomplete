{
    "version": 2,
    "builds": [
        {
            "src": "./server.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server.py"
        }
    ]
}